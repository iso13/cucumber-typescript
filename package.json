{
  "name": "cucumber-protractor",
  "version": "3.0.0",
  "license": "MIT",
  "description": "To demonstrate protractor cucumber tests for angular/non angular apps",
  "keywords": [
    "protractor",
    "cucumber",
    "typescript",
    "angular",
    "angularjs",
    "testing",
    "behaviour driven development",
    "bdd",
    "selenium",
    "blue-harvest",
    "gherkin"
  ],
  "main": "index.js",
  "scripts": {
    "commit": "commit",
    "build": "tsc",
    "clean": "rimraf dist/",
    "clean-build": "npm run clean && npm run build",
    "protractor": "tsc && gulp cleanJsonReport && protractor dist/config/config.js",
    "protractor:debug": "tsc && gulp cleanJsonReport && node --inspect-brk ./node_modules/.bin/protractor dist/config/config.js",
    "gherkin-lint": "gherkin-lint",
    "eslint": "eslint applicationObjects/ config/ helpers/ pages/ step_definitions/ support/ testData/ --ext .ts,.js,.json --no-fix --debug",
    "eslint:fix": "eslint applicationObjects/ config/ helpers/ pages/ step_definitions/ support/ testData/ --ext .ts,.js,.json --fix --debug"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run build && npm run eslint && gherkin-lint"
    }
  },
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/forms": "^6.1.7",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.7",
    "moment": "^2.24.0",
    "rxjs": "^6.3.2",
    "typescript-logging": "0.6.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@types/cucumber": "^4.0.4",
    "@types/fs-extra": "^7.0.0",
    "@types/gulp": "^4.0.0",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.9.4",
    "@types/selenium-webdriver": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "aws-sdk": "^2.375.0",
    "blue-harvest": "0.2.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.5.0",
    "cucumber": "^5.0.1",
    "cucumber-pretty": "^1.4.4",
    "deep-equal-in-any-order": "^1.0.13",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^8.0.1",
    "faker": "^4.1.0",
    "@types/faker": "^4.1.12",
    "gherkin-lint": "^3.0.3",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-typescript": "^5.0.1",
    "gulp-watch": "^5.0.1",
    "husky": "^3.1.0",
    "inversify": "^5.0.1",
    "lodash": "^4.17.15",
    "log4js": "^4.1.1",
    "prettier": "^1.19.1",
    "protractor": "^5.4.1",
    "protractor-cucumber-framework": "^6.1.1",
    "protractor-image-comparison": "^3.9.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.6.2",
    "ts-xlsx": "^0.0.11",
    "typescript": "^3.7.5",
    "xlsx": "^0.14.1",
    "protractor-retry": "^1.2.12"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": "https://bitbucket.org/limelighthealth/limelighte2e"
}
